{
  "name": "clean-tablet",
  "private": true,
  "version": "2.0.0",
  "description": "update of clone of Blank Slate, a party game",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 && echo 'Linted!'",
    "preview": "vite preview",
    "server": "go run server/main.go",
    "cover": "go tool cover -html c.out",
    "test:app": "go test -v github.com/jamessouth/clean-tablet/server/app",
    "test:app:coverage": "go test -coverprofile c.out github.com/jamessouth/clean-tablet/server/app",
    "test:clients": "go test -v github.com/jamessouth/clean-tablet/server/clients",
    "test:clients:coverage": "go test -coverprofile c.out github.com/jamessouth/clean-tablet/server/clients",
    "test:data:words": "go test -v github.com/jamessouth/clean-tablet/server/data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/clean-tablet.git"
  },
  "author": "james south",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamessouth/clean-tablet/issues"
  },
  "homepage": "https://github.com/jamessouth/clean-tablet#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "vite": "^5.4.6"
  }
}
